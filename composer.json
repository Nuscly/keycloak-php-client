{
    "name": "alma-medical-imaging/keycloak-client",
    "description": "A PHP client for Keycloak REST API",
    "keywords": ["keycloak", "api", "client"],
    "type": "library",
    "require": {
        "symfony/http-client": "^5.0",
        "psr/simple-cache": "^1.0",
        "symfony/cache": "^5.0",
        "stevenmaguire/oauth2-keycloak": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "AlmaMedical\\KeycloakClient\\": "src"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
